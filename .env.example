# Cornell Course Navigator - Environment Configuration
# Copy this file to .env.local for local development

# FastAPI Gateway URL
FASTAPI_BASE_URL=http://localhost:8000

# Neo4j Configuration (for FastAPI service)
NEO4J_URI=neo4j+s://xxxxx.databases.neo4j.io
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-generated-password

# Qdrant Configuration (for FastAPI service)
QDRANT_URL=https://xxxxx.us-east-1-0.aws.cloud.qdrant.io:6333
QDRANT_API_KEY=your-api-key

# OpenAI Configuration (for FastAPI service)
OPENAI_API_KEY=sk-your-openai-api-key

# Development Settings
ENVIRONMENT=development
USE_MOCK_SERVICES=false
LOG_LEVEL=info