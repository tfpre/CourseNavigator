  "RAGRequest": {
    "$defs": {
      "SearchMode": {
        "description": "Search mode for different types of queries",
        "enum": [
          "semantic",
          "graph_aware",
          "prereq_path"
        ],
        "title": "SearchMode",
        "type": "string"
      }
    },
    "description": "Request for RAG with graph context",
    "example": {
      "include_graph_context": true,
      "max_prerequisite_depth": 3,
      "mode": "graph_aware",
      "query": "machine learning courses with programming prerequisites",
      "top_k": 10
    },
    "properties": {
      "query": {
        "maxLength": 500,
        "minLength": 1,
        "title": "Query",
        "type": "string"
      },
      "mode": {
        "$ref": "#/$defs/SearchMode",
        "default": "graph_aware"
      },
      "top_k": {
        "default": 10,
